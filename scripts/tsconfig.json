{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["esnext"],
		"strict": true,
		"allowJs": false,
		"baseUrl": "scripts",
		"module": "CommonJS",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"noEmit": false,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
	}
}
